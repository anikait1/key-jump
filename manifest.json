{
  "manifest_version": 3,
  "name": "Keyboard Hints",
  "version": "1.1.0",
  "description": "Vimium-style keyboard hints for any website",
  "permissions": [],
  "action": {
    "default_popup": "src/popup.html",
    "default_title": "Keyboard Hints"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["dist/content.js"],
      "run_at": "document_idle"
    }
  ],
  "icons": {}
}
